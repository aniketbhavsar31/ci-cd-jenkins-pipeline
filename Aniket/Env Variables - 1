pipeline{
  agent any
  environment{
    name = "Aniket"
    surname = "Bhavsar"
    test = "This is test"
  }
  stages{
    environment{
      test1 = "This is test1"
    }
    stage("Print name"){
      steps{
      echo "My name is ${env.name} ${env.surname}"
      echo "This is ${env.test} and ${env.test1}"
      }
    }
  }
}
